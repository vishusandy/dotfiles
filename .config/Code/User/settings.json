    "better-comments.highlightPlainText": true,
    "better-comments.tags": [
        {
            "tag": "!!",
            "color": "#ff790a",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "!-",
            "color": "#ec540e",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "!_",
            "color": "#ff590d",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "==",
            "color": "#FF0055",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#343A41",
            "bold": true,
            "italic": false
        },
        {
            "tag": "??",
            "color": "#00D0AF",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#343A41",
            "bold": false,
            "italic": false
        },
        {
            "tag": "::",
            "color": "#2DE2FF",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#343A41",
            "bold": false,
            "italic": true
        },
        { // comments starting with TODO
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        { // comments that are a continuation of a previous TODO comment
            "tag": "&",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        { // comments that are a continuation of a previous TODO comment
            "tag": "||",
            "color": "#E0E0E0",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        { // comments that are a continuation of a previous TODO comment
            "tag": "TODO:",
            "color": "#ff7300",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "**",
            "color": "#00FFD7",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        // CUSTOM TAGS
        //  Triangle Color Picker:
        //    http://timbaumann.info/colortriangle/
        {
            "tag": "//",
            "color": "#707070",
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "---",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": "++",
            "color": "#62EA00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "@@@@",
            "color": "#E5007D",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": "@@@",
            "color": "#E5007D",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        { // This (the @@ tag) must be after the @@@ tag in order to funciton properly
            "tag": "@@",
            "color": "#ff008c",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "!>>", // the settings for !>> have to come before !> otherwise it doesn't show both of them
            "color": "#D3CDC1",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#33373b",
            "bold": true,
            "italic": false
        },
        {
            "tag": "\\>", // this evaluates to \> and not \\> due to character escapes
            "color": "#f5ddb0",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#33373b",
            "bold": true,
            "italic": false
        },
        {
            "tag": "!>",
            "color": "#CABEA7",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#33373b",
            "bold": true,
            "italic": false
        },
        {
            "tag": ">>",
            "color": "#CCC",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#33373b",
            "bold": true,
            "italic": false
        },
        {
            "tag": "->>",
            "color": "#F0F0F0",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#33373b",
            "bold": false,
            "italic": true
        },
        {
            "tag": ",,",
            "color": "#30AFFF",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "$$",
            "color": "#CA5DE5",
            "backgroundColor": "#343A41",
            "strikethrough": false,
            "underline": false,
            "bold": false,
            "italic": false
        },
        {
            "tag": "^^",
            "color": "#FFED00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
        //
        //
        //      this is a normal comment (my theme uses #A39C8E as comment foreground)
        // @@   heading
        // @@@  sub-heading
        // @@@@ sub-heading 2
        // !!   this is important
        // !_   this is an important [list] item header
        // !-   this is an important [list] item
        // ==   this is REALLY important
        // **   this is cyan
        // __   this is striked out
        // //   this is old
        // ??   this is a note or documention
        // ::   https://this-is-a.url.com/
        // ,,   I'm blue, da ba dee da ba daa
        // ^^   this is yellow
        // ..   this is kinda purple pink
        // $$   this is purple and I love it
        // \\         *To-do*
        // <<         *To-do*
        // +++  this line was added (diff)
        // ---  this was removed (diff)
        //TODO: this is a to-do list
        // &    this is a todo list
        //!>    important code snippet (like a snippet heading)
        //!>>   important code snippet (like a snippet heading)
        //\>    code snippet comment/description
        //*>    code snippet comment/description
        // >>   this-is-a.code(snippet)
        // ->>  this is terminal output, or text  output from code
        //TODO: Not finished yet
        //& change colors:
        // change ?? to orange (like !!), current cyan color is nice but not for reading documentation
        //& add color for the following:
        // ||   *To-do*
        // \\   *To-do*
        // <<   *To-do*
        //& add the following items:
        // ?_   documentation item, darker (or lighter) than ??
        // !==  like important == but more important?
        // *>   not sure what this is, or if it should be added
        // >_   code output
        // !=   (not sure)
        //& make copies of:
        // !>  as  |> 
        // !>> as  |>>
    ],
