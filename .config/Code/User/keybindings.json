// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+t",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+alt+escape",
        "command": "workbench.action.openProcessExplorer"
    },
    {
        "key": "alt+a",
        "command": "bracket-pair-colorizer-2.expandBracketSelection"
    },
    {
        "key": "ctrl+numpad3",
        "command": "editor.action.addCursorsToBottom"
    },
    {
        "key": "ctrl+numpad9",
        "command": "editor.action.addCursorsToTop"
    },
    {
        "key": "alt+t",
        "command": "editor.action.transpose",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+right",
        "command": "cursorWordPartRight",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+left",
        "command": "cursorWordPartLeft",
        "when": "editorTextFocus"
    },
    
    {
        "key": "ctrl+shift+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },    
    {
        "key": "alt+shift+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+shift+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+z",
        "command": "cursorRedo"
    },
    {
        "key": "alt+z",
        "command": "cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.duplicateSelection",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+z",
        "command": "editor.action.toggleWordWrap"
    },
    {
        "key": "alt+z",
        "command": "-editor.action.toggleWordWrap"
    },
    {
        "key": "alt+meta+space",
        "command": "editor.action.inspectTMScopes"
    },
    {
        "key": "alt+meta+m",
        "command": "editor.action.formatSelection.multiple"
    },
    {
        "key": "shift+alt+d",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "ctrl+shift+i",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-workbench.action.terminal.scrollUp",
        "when": "terminalFocus && terminalProcessSupported"
    },
    {
        "key": "shift+alt+i",
        "command": "-editor.action.insertCursorAtEndOfEachLineSelected",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "-editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k ctrl+c",
        "command": "-editor.action.addCommentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+[IntlBackslash]",
        "command": "-editor.action.autoFix",
        "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)quickfix\\b/"
    },
    {
        "key": "shift+alt+.",
        "command": "-editor.action.autoFix",
        "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)quickfix\\b/"
    },
    {
        "key": "shift+alt+h",
        "command": "-references-view.showCallHierarchy",
        "when": "editorHasCallHierarchyProvider"
    },
    {
        "key": "ctrl+f2",
        "command": "-editor.action.changeAll",
        "when": "editorTextFocus && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k m",
        "command": "-workbench.action.editor.changeLanguageMode"
    },
    {
      "key": "ctrl+shift+[",
      "command": "-editor.fold",
      "when": "editorTextFocus && foldingEnabled"
    },
    {
      "key": "ctrl+shift+[",
      "command": "editor.action.selectToBracket",
      "when": "editorLangId != 'markdown'"
    },
    {
      "key": "ctrl+shift+[",
      "command": "-notebook.fold",
      "when": "notebookEditorFocused && !inputFocus && activeEditor == 'workbench.editor.notebook'"
    },
    {
      "key": "ctrl+shift+[IntlBackslash]",
      "command": "-breadcrumbs.focusAndSelect",
      "when": "breadcrumbsPossible"
    },
    {
      "key": "ctrl+alt+f",
      "command": "-domqs.find",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+alt+m",
      "command": "-domqs.nextSelection",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+alt+p",
      "command": "-domqs.previousSelection",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+alt+a",
      "command": "-domqs.selectAll",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+alt+c",
      "command": "-hexdump.copyAsFormat",
      "when": "editorLangId == 'hexdump'"
    },
    {
      "key": "ctrl+shift+alt+end",
      "command": "editor.action.insertCursorAtEndOfEachLineSelected"
    },
    {
      "key": "ctrl+alt+]",
      "command": "editor.action.reindentselectedlines",
      "when": "editorTextFocus && !jupyter.hascodecells"
    },
    {
      "key": "ctrl+alt+right",
      "command": "editor.action.moveCarretRightAction",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "ctrl+alt+left",
      "command": "editor.action.moveCarretLeftAction",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "ctrl+alt+s",
      "command": "workbench.action.showAllSymbols"
    },
    {
      "key": "ctrl+t",
      "command": "-workbench.action.showAllSymbols"
    },
    {
      "key": "ctrl+alt+space",
      "command": "editor.action.trimTrailingWhitespace",
      "when": "editorTextFocus && !editorReadonly && !suggestWidgetVisible"
    },
    {
      "key": "ctrl+k ctrl+x",
      "command": "-editor.action.trimTrailingWhitespace",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "alt+space",
      "command": "workbench.action.pinEditor",
      "when": "!activeEditorIsPinned"
    },
    {
      "key": "ctrl+k shift+enter",
      "command": "-workbench.action.pinEditor",
      "when": "!activeEditorIsPinned"
    },
    {
      "key": "alt+space",
      "command": "workbench.action.unpinEditor",
      "when": "activeEditorIsPinned"
    },
    {
      "key": "ctrl+k shift+enter",
      "command": "-workbench.action.unpinEditor",
      "when": "activeEditorIsPinned"
    },
    {
      "key": "ctrl+shift+numpad_subtract",
      "command": "editor.action.smartSelect.shrink",
      "when": "editorTextFocus"
    },
    {
      "key": "shift+alt+left",
      "command": "-editor.action.smartSelect.shrink",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+shift+numpad_add",
      "command": "editor.action.smartSelect.expand",
      "when": "editorTextFocus"
    },
    {
      "key": "shift+alt+right",
      "command": "-editor.action.smartSelect.expand",
      "when": "editorTextFocus"
    },
    {
      "key": "ctrl+alt+up",
      "command": "workbench.action.moveEditorToPreviousGroup"
    },
    {
      "key": "ctrl+alt+left",
      "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
      "key": "ctrl+alt+down",
      "command": "workbench.action.moveEditorToNextGroup"
    },
    {
      "key": "ctrl+alt+right",
      "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
      "key": "ctrl+alt+meta+up",
      "command": "workbench.action.moveEditorToAboveGroup"
    },
    {
      "key": "ctrl+alt+meta+down",
      "command": "workbench.action.moveEditorToBelowGroup"
    },
    {
      "key": "ctrl+alt+meta+left",
      "command": "workbench.action.moveEditorToLeftGroup"
    },
    {
      "key": "ctrl+alt+meta+right",
      "command": "workbench.action.moveEditorToRightGroup"
    },
    {
      "key": "ctrl+tab",
      "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
    },
    {
      "key": "ctrl+shift+tab",
      "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
    },
    {
      "key": "ctrl+tab",
      "command": "workbench.action.nextEditorInGroup"
    },
    {
      "key": "ctrl+k ctrl+pagedown",
      "command": "-workbench.action.nextEditorInGroup"
    },
    {
      "key": "ctrl+shift+tab",
      "command": "workbench.action.previousEditorInGroup"
    },
    {
      "key": "ctrl+k ctrl+pageup",
      "command": "-workbench.action.previousEditorInGroup"
    }
]
